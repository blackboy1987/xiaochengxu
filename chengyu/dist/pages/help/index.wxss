@import "./../../remax-styles.wxss";
page {
    color: #fff;
    overflow-x: hidden;
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
}

.wlist {
    margin: 40rpx;
}

.wlistitem {
    margin: 30rpx 0;
}

.wtitle {
    background-color: #fff;
    color: #333;
    height: 90rpx;
    line-height: 90rpx;
    font-size: 30rpx;
    padding-left: 50rpx;
    border-radius: 10rpx;
}

.wtitleactive {
    color: #a3a3a3;
    border-bottom-right-radius: 0;
    border-bottom-left-radius: 0;
    border-bottom: 2rpx solid #a3a3a3;
}

.wmiaoshu {
    background-color: #fff;
    color: #000;
    padding: 50rpx;
    font-size: 26rpx;
    line-height: 46rpx;
}

.wbtn {
    text-align: center;
    background-image: linear-gradient(-90deg,#705ade 1%,#a038fe);
    width: 90%;
    height: 80rpx;
    line-height: 80rpx;
    font-size: 30rpx;
    margin: 0 auto;
    border-radius: 50rpx;
}

.wad {
    padding: 20rpx;
}
