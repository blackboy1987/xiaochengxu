
page {
    background: #fff;
}

.maina {
    position: relative;
}

.maina .maina_bg image {
    width: 100%;
}

.maina .box1 {
    position: absolute;
    top: 15%;
    text-align: center;
    width: 100%;
    color: #fff;
    font-size: 50rpx;
    line-height: 120rpx;
}

.maina .box1 text {
    background-image: url(data:image/png;base64,);
    width: 80rpx;
    height: 120rpx;
    display: inline-block;
    background-size: 100% 100%;
    margin: 0 10rpx;
    color: #ff9114;
    font-weight: 700;
}

.maina .box2 {
    position: absolute;
    top: 45%;
    background: rgba(255,137,1,.8);
    width: 80%;
    left: 50%;
    margin-left: -40%;
    border-radius: 100rpx;
    line-height: 100rpx;
    color: #fff;
    text-align: center;
    font-size: 15rpx;
}

.maina .box2 image {
    width: 40rpx;
    height: 40rpx;
    display: inline-block;
    vertical-align: top;
    margin-top: 30rpx;
    margin-right: 10rpx;
}

.mainb {
    background: #fff;
    box-shadow: 4rpx 8rpx 40rpx #ccc;
    border-radius: 20rpx;
    overflow: hidden;
    padding: 60rpx 20rpx;
    margin: -200rpx 20rpx 20rpx;
    z-index: 9;
    position: relative;
}

.mainb .box {
    width: 22.75%;
    float: left;
    background: #f0f0f0;
    box-sizing: border-box;
    margin-right: 3%;
    margin-bottom: 20rpx;
    border-radius: 10rpx;
    text-align: center;
    font-size: 24rpx;
    padding: 20rpx 0;
}

.mainb .box.active {
    background: #fe4c5a;
    color: #fff;
}

.mainb .box:nth-child(4),.mainb .box:nth-child(7) {
    margin-right: 0;
}

.mainb .box image {
    width: 60rpx;
    height: 60rpx;
    display: block;
    margin: 6rpx auto;
}

.mainb .box2 {
    width: 325rpx;
    height: 188rpx;
    display: flex;
    padding-top: 50rpx;
}

.mainb .box2 .text {
    height: 95rpx;
    margin-left: 50rpx;
    line-height: 47rpx;
}

.mainb .box2 image {
    width: 88rpx;
    height: 95rpx;
    margin: 0 0 0 30rpx;
}

.mainb .btn {
    width: 400rpx;
    background: #ff9114;
    line-height: 80rpx;
    text-align: center;
    border-radius: 100rpx;
    color: #fff;
    margin: 20rpx auto 0;
}
