::-webkit-scrollbar {
    width: 0;
    height: 0;
    color: transparent;
}

#head {
    position: fixed;
    left: 0;
    right: 0;
    top: 0;
    z-index: 50;
    margin-bottom: 10rpx;
    background: #fff;
}

.header {
    height: 44rpx;
    background-color: #d42e2e;
    padding: 0 12rpx;
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.head-bd {
    width: 100%;
    height: 35rpx;
    overflow: hidden;
    margin-right: auto;
    margin-left: auto;
    position: relative;
}

#nav {
    height: 49rpx;
    padding-top: 6rpx;
    background: #fff;
    text-align: center;
    font-size: 0;
    position: relative;
}

.search_bar {
    width: 74rpx;
    height: 100%;
    display: inline-block;
    vertical-align: top;
    background-image: url("data:image/png;base64,");
    background-size: 18rpx 18rpx;
    background-repeat: no-repeat;
    background-position: center;
    position: relative;
    z-index: 2;
}

.nav_bar {
    position: absolute;
    right: 34rpx;
    top: 9rpx;
    width: 20rpx;
    height: 18rpx;
}

.ui-m-overflowX {
    position: absolute;
    left: 12rpx;
    right: 34rpx;
    display: inline-block;
    width: 700rpx;
    overflow-x: auto;
    -webkit-overflow-scrolling: touch;
}

.ui-m-overflowX-content {
    width: 1600rpx;
    overflow: hidden;
    font-size: 18rpx;
}

.ui-m-overflowX-main {
    float: left;
}

.ui-m-overflowX-main view {
    float: left;
    height: 29rpx;
}

#nav .ui-m-overflowX-main text {
    float: left;
    padding: 0 8rpx;
    height: 23rpx;
    line-height: 23rpx;
    color: #444;
    font-weight: bold;
}

#nav .ui-m-overflowX-main .active text {
    color: #d42e2e;
}

#nav .active {
    position: relative;
}

#nav .active:after {
    content: '';
    position: absolute;
    width: 40rpx;
    height: 6rpx;
    border-radius: 40rpx;
    background: #ff4a45;
    bottom: 0;
    left: 50%;
    transform: translate(-50%,0);
}
.foot {
    padding-top: 8rpx;
    padding-bottom: 8rpx;
    color: #666;
    text-align: center;
}

.duration {
    display: none;
    position: absolute;
    right: 4%;
    bottom: 4%;
    padding-right: 4rpx;
    padding-left: 4rpx;
    background-color: rgba(0,0,0,0.5);
    color: #dcdbdb;
}

.video-related {
    padding: 8rpx 2%;
}

.playerWrap {
    position: relative;
    width: 100%;
    height: 0;
    padding-bottom: 56.25%;
}

#foot {
    padding: 30rpx 0;
    background: #f6f6f6;
    text-align: center;
    color: #999;
    font-size: 12rpx;
}

.player {
    z-index: 20;
    position: fixed;
    left: 0;
    right: 0;
    top: 0;
    width: 100%;
    height: 0;
    padding-bottom: 56.25%;
    background: black;
}

.player-video {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
}

.player-media {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 10;
}

.player-media .poster {
    position: absolute;
    top: 0;
    left: 0;
}

.player-media .info-title {
    position: absolute;
    top: 0;
    left: 0;
    width: 96%;
    padding: 4rpx 2%;
    background-color: rgba(0,0,0,0.5);
    color: #dcdbdb;
    font-size: 14rpx;
    z-index: 10;
    height: 22rpx;
    overflow: hidden;
    white-space: nowrap;
}

.player-play {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate3d(-50%,-50%,0);
    width: 54rpx;
    height: 54rpx;
    background: url(http://static01.baomihua.com/img/mobile/v3/play-current20161111.png);
    background-size: 190rpx 70rpx;
    background-position: -8rpx -8rpx;
    z-index: 15;
}

.scroll_wrap {
    position: fixed;
    left: 0;
    right: 0;
    bottom: 94rpx;
    overflow-y: auto;
    top: 56.25%;
}

.video_bottom {
    height: 0.94rem;
    overflow: hidden;
    position: relative;
    margin: 0 17rpx 10rpx 17rpx;
}

.video_bottom a {
    display: block;
}

.video_bottom .describe,.header-top .describe,#bodybottom .describe {
    position: absolute;
    right: 0;
    top: 0;
    font-size: 12rpx;
    color: #fff;
    background: rgba(0,0,0,0.15);
    padding: 1rpx 3rpx;
    border-radius: 0 0 0 4rpx;
    transform: scale(0.8,0.8);
    transform-origin: top right;
}

.video_infos_wrap {
    padding: 0px 17rpx 12rpx 17rpx;
    border-bottom: 6rpx solid #f4f5f6;
}

.more_details_wrap {
    margin: 16rpx 0 0 0;
    display: flex;
    align-items: center;
    color: #999999;
    font-size: 24rpx;
    line-height: 1;
    position: relative;
}

.more_details_wrap .swiper_container {
    width: 100%;
    padding-right: 36rpx;
    height: 22rpx;
    line-height: 22rpx;
}

.more_details_wrap .swiper_container .item {
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.more_details_wrap .more_close {
    position: absolute;
    right: 0;
    top: 4rpx;
    width: 26rpx;
    height: 26rpx;
}

.more_details_wrap .more_close image {
    width: 100%;
    height: 100%;
    line-height: 0;
    position: relative;
}

.more_details_wrap .more_close:before {
    content: '';
    display: block;
    position: absolute;
    top: -20rpx;
    right: -20rpx;
    bottom: -20rpx;
    left: -20rpx;
}

.video_title {
    font-size: 34rpx;
    color: #272727;
    font-weight: bold;
    margin-top: 16rpx;
}

.video_user {
    display: -webkit-box;
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    -ms-flex-align: center;
    align-items: center;
    -webkit-box-pack: justify;
    -ms-flex-pack: justify;
    justify-content: space-between;
    margin-top: 20rpx;
}

.video_user_info {
    display: inline-flex;
    align-items: center;
    flex: 1;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.video_user_info image {
    border-radius: 50%;
    width: 48rpx;
    height: 48rpx;
}

.video_user_info text {
    color: #777;
    font-size: 30rpx;
    margin-left: 22rpx;
    line-height: 2rpx;
}

.video_user .share_btn {
    overflow: visible;
    display: flex;
    justify-content: center;
    align-items: center;
    flex: 0 0 128rpx;
    padding: 0;
    width: 128rpx;
    height: 48rpx;
    background: #2bc846;
    font-size: 24rpx;
    color: #fff;
    border-radius: 52rpx;
    position: relative;
}

.video_user .share_btn image {
    display: inline-block;
    margin-left: 8rpx;
    width: 28rpx;
    height: 24rpx;
}

.video_user .share_btn:before {
    content: '';
    position: absolute;
    top: 0;
    right: 0;
    width: 12rpx;
    height: 12rpx;
    background: #ff4a45;
    border-radius: 50%;
}

.video_user button::after {
    display: none;
}
.flow-link {
    position: relative;
    margin: 0 34rpx;
}
.flow-linka {
    display: block;
    padding: 30rpx 0;
}

.flow-link:after {
    position: absolute;
    content: "";
    height: 2rpx;
    left: 0;
    right: 0;
    bottom: 0;
    border-bottom: 2rpx solid #eee;
}

#pic {
    float: left;
    font-size: 0;
    width: 230rpx;
    height: 130rpx;
    overflow: hidden;
    position: relative;
}

#img {
    width: 230rpx;
    top: 50%;
}
.info {
     margin-left: 250rpx;
 }
.info-title {
    margin-bottom: 6rpx;
    font-size: 34rpx;
    line-height: 1.3;
    height: 88rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    color: #222;
}
.info-count {
    font-size: 24rpx;
    color: #9c9c9c;
}
.play-fixed-btn {
    position: fixed;
    right: 30rpx;
    bottom: 112rpx;
    width: 96rpx;
    height: 96rpx;
    background: #fff;
    border-radius: 50%;
    display: flex;
    justify-content: center;
    align-items: center;
    box-shadow: 0 0 32rpx rgba(212,210,211,.8);
}

.play-fixed-btn image {
    width: 46rpx;
    height: 48rpx;
}
.wrap,.circle,.percent{
    position: absolute;
    width: 96rpx;
    height: 96rpx;
    border-radius: 50%;
}
.wrap{
    position: fixed;
    right: 30rpx;
    bottom: 112rpx;
    background-color: #ccc;
    display: flex;
    justify-content: center;
    align-items: center;
    box-shadow: 0 0 32rpx rgba(212,210,211,.8);
}
.circle{
    box-sizing: border-box;
    border:20rpx solid #ccc;
    clip:rect(0,96rpx,96rpx,48rpx);
}
.clip-auto{
    clip:rect(auto, auto, auto, auto);
}
.percent{
    box-sizing: border-box;
    top:-20rpx;
    left:-20rpx;
}
.left{
    transition:transform ease;
    border:20rpx solid blue;
    clip: rect(0,48rpx,96rpx,0);
    transform:rotate(80deg);
}
.right{
    border:20rpx solid blue;
    clip: rect(0,96rpx,96rpx,48rpx);
}
.wth0{
    width:0;
}
.num{
    position: absolute;
    box-sizing: border-box;
    width: 80rpx;
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    font-size: 40rpx;
    border-radius: 50%;
    background-color: #fff;
    z-index: 1;
}
